<body class="sidebar-wide">
    <!-- Navbar -->
    <div class="navbar navbar-inverse" role="navigation">
        <div class="navbar-header">
            <a class="navbar-brand" href="#">
                @*<img src="~/Content/themes/Admin/images/logo.png" alt="Londinium">*@
                eSchool
            </a>
            <a class="sidebar-toggle"><i class="icon-paragraph-justify2"></i></a>
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-icons"><span class="sr-only">Toggle navbar</span><i class="icon-grid3"></i></button>
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".sidebar"><span class="sr-only">Toggle navigation</span><i class="icon-paragraph-justify2"></i></button>
        </div>
        <ul class="nav navbar-nav navbar-right collapse" id="navbar-icons">
        
            <li class="user dropdown"><a class="dropdown-toggle" data-toggle="dropdown">
                <img src="~/Content/themes/admin/images/demo/users/face1.png" alt=""><span>Mr.Admin</span><i class="caret"></i></a>
                <ul class="dropdown-menu dropdown-menu-right icons-right">
                    
                    <li><a href="#" ><i class="icon-user"></i>Change Password</a></li>
                    <li><a href="#"><i class="icon-exit"></i>Logout</a></li>
                </ul>
            </li>
        </ul>
    </div>
    <!-- /navbar -->
    <!-- Page container -->
    <div class="page-container">
        <!-- Sidebar -->
        <div class="sidebar collapse">
            <div class="sidebar-content">
                <!-- User dropdown -->
                <div class="user-menu dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        
                        <img src="~/Content/themes/admin/images/demo/users/face1.png" alt="">
                        <div class="user-info">Mr.Admin <span>admin</span></div>
                    </a>
                    <div class="popup dropdown-menu dropdown-menu-right">
                        <div class="thumbnail">
                            <div class="thumb">
                                <img alt="" src="~/Content/themes/admin/images/demo/users/face1.png">
                                <div class="thumb-options"><span><a href="#" class="btn btn-icon btn-success"><i class="icon-pencil"></i></a>
                                    </span></div>
                            </div>
                            <div class="caption text-center">
                                <h6>Mr.Admin <small>admin</small></h6>
                            </div>
                        </div>
                  
                    </div>
                </div>
                <!-- /user dropdown -->
                <!-- Main navigation -->
                <ul class="navigation">
                  
                   <li><a href="#" class="expand"><span>Employee</span> <i class="icon-user"></i></a>
                        <ul>
                            <li><a href="@Url.Action("Create","Home")">Add Employee</a></li>
                            <li><a href="@Url.Action("Index","Home")">Employee list</a></li>
                            
                        </ul>
                    </li>
      
                   
                </ul>
                <!-- /main navigation -->
            </div>
        </div>
        <!-- /sidebar -->
        <!-- Page content -->
        <div class="page-content">
       
            <br />
            <!-- /page header -->
            <!-- Breadcrumbs line -->
            <div class="breadcrumb-line">
                <ul class="breadcrumb">

                    <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                    <li class="active">Dashboard</li>
                </ul>
                <div class="visible-xs breadcrumb-toggle"><a class="btn btn-link btn-lg btn-icon" data-toggle="collapse" data-target=".breadcrumb-buttons"><i class="icon-menu2"></i></a></div>
           
            </div>
            <!-- /breadcrumbs line -->
            @RenderBody()
            <!-- /page tabs -->
            <!-- Footer -->
            @Html.Partial("_FooterPartial")
            <!-- /footer -->
        </div>
        <!-- /page content -->
    </div>
    <style>
        .ui-dialog {
        z-index:9999;
        }
    </style>
      <div id="dialog" ></div>
<script type="text/javascript">
    $(document).ready(function () {
        $(function () {
            $("#dialog").dialog({

                autoOpen: false,
                modal: true,
                minHeight: 200,
                minWidth: 520,
                show: {
                    effect: "blind",
                    duration: 1000
                },   
                hide: {
                    effect: "fold",
                    duration: 1000
                }
            });
        });

    

    });
  </script>
    <!-- /page container -->
   
    @RenderSection("scripts", required: false)

</body>
</html>
